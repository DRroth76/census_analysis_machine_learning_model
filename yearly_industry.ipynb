   "source": [
    "for transportation_index in range(len(transportation_type_list)):  # Loop over the index of industry_list\n",
    "    transportation_int = str(transportation_index + 3).zfill(2)  # Construct industry_int based on industry_index\n",
    "    try:\n",
    "        transportation_data = pd.DataFrame()  # Initialize as an empty DataFrame for each industry\n",
    "        print(f\"Fetching data for income with means of transportation {transportation_type_list[transportation_index]}...\")\n",
    "        for year in range(2014, 2023):\n",
    "            print(f\"Fetching data for year {year}...\")\n",
    "            url = f'https://api.census.gov/data/{year}/acs/acs1?get=NAME,B08121_0{transportation_int}E&for=place:*&key={census_api}'\n",
    "            response = requests.get(url)\n",
    "            if response.status_code == 200:\n",
    "                json_response = response.json()\n",
    "                data = pd.DataFrame(json_response[1:], columns=json_response[0])\n",
    "                data = data.rename(columns={f'B08121_0{transportation_int}E': 'Median Income'})\n",
    "                data = data.dropna()\n",
    "                data = data.loc[data['Median Income'] != '0']\n",
    "                data['Means of Transportation'] = transportation_type_list[transportation_index]\n",
    "                data['Median Income'] = data['Median Income'].astype(int)\n",
    "                data = data.sort_values('Median Income', ascending=False)\n",
    "                data['Year'] = str(year)\n",
    "                transportation_data = pd.concat([transportation_data, data], ignore_index=True)  # Concatenate data for each year\n",
    "                print(f\"Data for year {year} fetched successfully.\")\n",
    "                print(f\"Size of transportation data: {transportation_data.shape}\")\n",
    "                print(f\"Size of data after concatenation: {data.shape}\")\n",
    "            else:\n",
    "                print(f\"Error fetching data for year {year}: Status code {response.status_code}\")\n",
    "        transportation_earnings = pd.concat([transportation_earnings, transportation_data], ignore_index=True)  # Concatenate data for each industry\n",
    "        print(f\"Size of transporation dataframe: {transportation_earnings.shape}\")\n",
    "    except Exception as e:\n",
    "        print(f'Error fetching data for transportation {transportation_type_list[transportation_index]}: {e}')\n",
    "print(transportation_earnings)"
