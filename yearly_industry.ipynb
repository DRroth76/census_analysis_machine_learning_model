    "for rent_index in range(len(rent_type_list)):  # Loop over the index of industry_list\n",
    "    rent_int = str(rent_index + 3).zfill(2)  # Construct industry_int based on industry_index\n",
    "    try:\n",
    "        rent_data = pd.DataFrame()  # Initialize as an empty DataFrame for each industry\n",
    "        print(f\"Fetching data for rent values {rent_type_list[rent_index]}...\")\n",
    "        for year in range(2014, 2023):\n",
    "            print(f\"Fetching data for year {year}...\")\n",
    "            url = f'https://api.census.gov/data/{year}/acs/acs1?get=NAME,B25063_0{rent_int}E&for=place:*&key={census_api}'\n",
    "            response = requests.get(url)\n",
    "            if response.status_code == 200:\n",
    "                json_response = response.json()\n",
    "                data = pd.DataFrame(json_response[1:], columns=json_response[0])\n",
    "                data = data.rename(columns={f'B25063_0{rent_int}E': 'Total Households'})\n",
    "                data = data.dropna()\n",
    "                data = data.loc[data['Total Households'] != '0']\n",
    "                data['Rent Amount'] = rent_type_list[rent_index]\n",
    "                data['Total Households'] = data['Total Households'].astype(int)\n",
    "                data = data.sort_values('Total Households', ascending=False)\n",
    "                data['Year'] = str(year)\n",
    "                rent_data = pd.concat([rent_data, data], ignore_index=True)  # Concatenate data for each year\n",
    "                print(f\"Data for year {year} fetched successfully.\")\n",
    "                print(f\"Size of rent data: {rent_data.shape}\")\n",
    "                print(f\"Size of data after concatenation: {data.shape}\")\n",
    "            else:\n",
    "                print(f\"Error fetching data for year {year}: Status code {response.status_code}\")\n",
    "        rent_df = pd.concat([rent_df, rent_data], ignore_index=True)  # Concatenate data for each industry\n",
    "        print(f\"Size of rent dataframe: {rent_df.shape}\")\n",
    "    except Exception as e:\n",
    "        print(f'Error fetching data for rent values {rent_type_list[rent_index]}: {e}')\n",
    "print(rent_df)\n"
   ]
  },
